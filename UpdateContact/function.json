{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": ["post"]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "inputDocument",
      "databaseName": "my-database",
      "collectionName": "my-container1",
      "connectionStringSetting": "CosmosDbConnectionStr",
      "sqlQuery": "select * from c where c.id ={id} AND c.userId={userId}"
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "outputDocument",
      "databaseName": "my-database",
      "collectionName": "my-container1",
      "createIfNotExists": false,
      "connectionStringSetting": "CosmosDbConnectionStr"
    }
  ],
  "scriptFile": "../dist/UpdateContact/index.js"
}
