{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": ["get", "post"]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "outputDocument",
      "databaseName": "my-database",
      "collectionName": "my-container1",
      "createIfNotExists": false,
      "connectionStringSetting": "CosmosDbConnectionStr"
    }
  ],
  "scriptFile": "../dist/createContacts/index.js"
}
